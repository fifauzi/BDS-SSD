{
  "expo": {
    "name": "TrackerApp",
    "slug": "TrackerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logow.jpg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "trackerapp",
    "splash": {
      "image": "assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F54A38"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#F54A38",
        "dark": {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#F54A38"
        }
      },
      "bundleIdentifier": "com.trackerapp"
    },
    "android": {
      "package": "com.trackerapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logow.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-permissions",
        {
          "ios": ["LOCATION_WHEN_IN_USE"],
          "android": ["android.permission.ACCESS_FINE_LOCATION"]
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "fifauzi"
  }
}
